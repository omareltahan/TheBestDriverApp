<?xml version="1.0" encoding="utf-8"?>
<ScrollView android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:background="@drawable/bg_splash"
        android:padding="20sp"
        android:layout_height="match_parent"
        android:gravity="center|top"
        tools:context=".Activities.LoginActivity">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_gravity="start"
            android:layout_marginBottom="20sp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_marginEnd="20sp"
                android:id="@+id/back"
                android:src="@drawable/back"
                android:layout_height="wrap_content"/>
            <TextView
                android:text="@string/edit_profile"
                android:layout_width="wrap_content"
                android:textColor="@color/blackcolor"
                android:textStyle="bold"
                android:textSize="32sp"
                android:layout_height="wrap_content"/>
        </LinearLayout>

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            app:cardCornerRadius="20sp"
            android:padding="20sp"
            app:cardBackgroundColor="@color/whitecolor"
            android:layout_marginBottom="20sp"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:gravity="center"
                android:padding="15sp"
                android:layout_height="wrap_content">
                <ImageView
                    android:src="@drawable/plusshop"
                    android:layout_width="100sp"
                    android:layout_marginBottom="10sp"
                    android:id="@+id/logo"
                    android:layout_height="100sp"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:padding="7sp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:layout_height="wrap_content">
                    <EditText
                        android:layout_width="match_parent"
                        android:textStyle="bold"
                        android:textSize="17sp"
                        android:textColor="@color/blackcolor"
                        android:hint="Shop name"
                        android:background="@drawable/background_gray_color"
                        android:paddingStart="10sp"
                        android:paddingEnd="10sp"
                        android:id="@+id/name"
                        android:paddingTop="4sp"
                        android:layout_marginBottom="10sp"
                        android:paddingBottom="4sp"
                        android:layout_height="40sp"/>
                    <EditText
                        android:layout_marginBottom="10sp"
                        android:layout_width="match_parent"
                        android:textStyle="bold"
                        android:textSize="17sp"
                        android:textColor="@color/blackcolor"
                        android:background="@drawable/background_gray_color"
                        android:paddingStart="10sp"
                        android:paddingEnd="10sp"
                        android:id="@+id/namear"
                        android:hint="اسم المتحر"
                        android:paddingTop="4sp"
                        android:paddingBottom="4sp"
                        android:layout_height="40sp"/>
                    <EditText
                        android:layout_width="match_parent"
                        android:textStyle="bold"
                        android:textSize="17sp"
                        android:textColor="@color/blackcolor"
                        android:hint="Shop Desc"
                        android:background="@drawable/background_gray_color"
                        android:paddingStart="10sp"
                        android:paddingEnd="10sp"
                        android:id="@+id/desc"
                        android:paddingTop="4sp"
                        android:layout_marginBottom="10sp"
                        android:paddingBottom="4sp"
                        android:lines="3"
                        android:layout_height="wrap_content"/>
                    <EditText
                        android:layout_width="match_parent"
                        android:textStyle="bold"
                        android:textSize="17sp"
                        android:textColor="@color/blackcolor"
                        android:background="@drawable/background_gray_color"
                        android:paddingStart="10sp"
                        android:paddingEnd="10sp"
                        android:id="@+id/descar"
                        android:hint="وصف المتحر"
                        android:paddingTop="4sp"
                        android:paddingBottom="4sp"
                        android:lines="3"

                        android:layout_height="wrap_content"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        app:cardCornerRadius="20sp"
        android:padding="20sp"
        app:cardBackgroundColor="@color/whitecolor"
        android:layout_marginBottom="20sp"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:padding="20sp"
        android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_height="wrap_content">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:src="@drawable/home"
                android:tint="@color/colorPrimaryDark"
                android:layout_marginEnd="10sp"
                android:layout_height="wrap_content"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <EditText
                    android:layout_width="match_parent"
                    android:textStyle="bold"
                    android:textSize="17sp"
                    android:textColor="@color/blackcolor"
                    android:hint="Shop Address"
                    android:background="@drawable/background_gray_color"
                    android:paddingStart="10sp"
                    android:paddingEnd="10sp"
                    android:id="@+id/address"
                    android:paddingTop="4sp"
                    android:layout_marginBottom="10sp"
                    android:paddingBottom="4sp"
                    android:lines="2"
                    android:layout_height="wrap_content"/>
                <EditText
                    android:layout_width="match_parent"
                    android:textStyle="bold"
                    android:textSize="17sp"
                    android:textColor="@color/blackcolor"
                    android:background="@drawable/background_gray_color"
                    android:paddingStart="10sp"
                    android:paddingEnd="10sp"
                    android:id="@+id/addressar"
                    android:hint="عنوان المتحر"
                    android:paddingTop="4sp"
                    android:paddingBottom="4sp"
                    android:lines="2"
                    android:layout_height="wrap_content"/>

            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_height="wrap_content">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/motorcycle"
            android:tint="@color/colorPrimaryDark"
            android:layout_marginEnd="10sp"
            android:layout_height="wrap_content"/>
            <EditText
                android:layout_width="match_parent"
                android:textStyle="bold"
                android:textSize="17sp"
                android:textColor="@color/blackcolor"
                android:hint="@string/delivery_fees"
                android:inputType="number"
                android:background="@drawable/background_gray_color"
                android:paddingStart="10sp"
                android:paddingEnd="10sp"
                android:id="@+id/fees"
                android:paddingTop="4sp"
                android:layout_marginBottom="10sp"
                android:paddingBottom="4sp"
                android:lines="2"
                android:layout_height="40sp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:orientation="horizontal"
            android:layout_marginTop="10sp"
            android:layout_height="wrap_content">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_gravity="center"
            android:src="@drawable/wage"
            android:tint="@color/colorPrimaryDark"
            android:layout_marginEnd="10sp"
            android:layout_height="wrap_content"/>
            <EditText
                android:layout_width="match_parent"
                android:textStyle="bold"
                android:textSize="17sp"
                android:textColor="@color/blackcolor"
                android:hint="@string/minimum_order"
                android:inputType="number"
                android:background="@drawable/background_gray_color"
                android:paddingStart="10sp"
                android:paddingEnd="10sp"
                android:id="@+id/minorder"
                android:paddingTop="4sp"
                android:layout_marginBottom="10sp"
                android:paddingBottom="4sp"
                android:lines="2"
                android:layout_height="40sp"/>
        </LinearLayout>

    </LinearLayout>
    </androidx.cardview.widget.CardView>
